<configuration>
                                 <!--相对路径：Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\Library.mdf;Integrated Security=True;Connect Timeout=30 -->
                                             <!--连接字符串相对路径阔能会出错，建议将AttachDbFilename更换成绝对路径！！！-->
    <connectionStrings>
        <add name="dbString" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\Library.mdf;Integrated Security=True;Connect Timeout=30"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />
    </system.web>

</configuration>
